	v = parseFloat(line);
	s = ( ( v - minv) / (maxv - minv) ) * 100.0;
	i = parseInt(s);
	c = lut[100 - i];
	printf("v: %.1f, s: %.1f, i: %d, c: %d\n", v, s, i, c);
}
