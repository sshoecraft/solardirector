var minv = 37.799999237060547;
var maxv = 58.099998474121094;

for(v = maxv; v >= minv-.1; v -= .1) {
	s = ( ( v - minv) / (maxv - minv) ) * 100.0;
	i = parseInt(s);
	s = ( ( v - minv) / (maxv - minv) ) * 100.0;
	i = parseInt(s);
	c = lut[100 - i];
	printf("v: %.1f, s: %.1f, i: %d, c: %d\n", v, s, i, c);
}
exit(0);

