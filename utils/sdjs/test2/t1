

var f = File("t1.dat");
if (!f) exit(1);

f.open("read");
i = 0;
var lut = [];
p = total = 0.0;
while(line = f.readln(88)) {
	printf("line: %s\n", line);
	if (++i > 10) break;
	n = parseFloat(line);
//	total += n;
	if (p) total += (p - n);
	p = n;
}
printf("d: %f\n", total);
step = total / 10;
printf("step: %f\n", step);
for(i=100.0; i > 84.99999; i -= step) {
	printf("%f\n", i);
}
