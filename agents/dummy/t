cmin=2.92
cmax=4.15
//min=41.0
min=cmin*14
//max=58.1
max=cmax*14
//s=((max - min) * .9)
s=(max - min);
printf("min: %f, max: %f, s: %f\n", min, max, s);
// spread = max - min
// vtg = min + (spread * pct)
// pct = (vgt - min) / spread
for(v=min; v < max+0.1; v += 0.1) {
	v=pround(v,1);
	p=(v - min) / s;
	p *= .9;
	printf("v: %f, p: %f\n", v, p);
}
